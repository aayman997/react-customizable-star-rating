{
  "name": "react-customizable-star-rating",
  "version": "0.1.2",
  "description": "React component to rate and view rating",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "author": "Ahmed Ayman",
  "engines": {
    "node": "18.16.0"
  },
  "keywords": [
    "react",
    "rating",
    "rating-stars",
    "stars"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aayman997/react-customizable-star-rating.git"
  },
  "scripts": {
    "start": "react-scripts start",
    "eject": "react-scripts eject",
    "lint": "eslint \"{**/*,*}.{js,ts,jsx,tsx}\"",
    "test": "jest --config jestconfig.json",
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "prepublishOnly": "npm test && npm run lint",
    "prepare": "npm run build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/user-event": "^13.5.0",
    "@types/node": "^16.18.38",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "devDependencies": {
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.3",
    "@typescript-eslint/eslint-plugin": "^5.0.0-alpha.42",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.44.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.6.1",
    "jest-canvas-mock": "^2.5.2",
    "jest-environment-jsdom": "^29.6.1",
    "ts-jest": "^29.1.1"
  }
}
